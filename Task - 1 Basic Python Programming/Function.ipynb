   "source": [
    "year = int(input(\"Enter a year\"))\n",
    "def is_leap(year):\n",
    "    leap = False\n",
    "    \n",
    "    if year % 4 == 0 and (year % 400 == 0 or year % 100 != 0):\n",
    "        leap = True    \n",
    "    return leap\n",
    "print(is_leap(year))"
