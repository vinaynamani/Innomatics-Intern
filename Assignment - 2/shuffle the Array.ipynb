   "source": [
    "def shuffle(nums,n):\n",
    "    shuffled_list = []\n",
    "    for i in range(0,len(nums)-n):\n",
    "        shuffled_list.extend([nums[i],nums[i+n]])\n",
    "    return shuffled_list"
   ]
  },
