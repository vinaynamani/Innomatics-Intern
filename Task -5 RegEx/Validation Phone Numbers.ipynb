    "import re\n",
    "pattern = \"[789]\\d{9}$\"\n",
    "N = int(input())\n",
    "for i in range(N):\n",
    "    string = input()\n",
    "    op = re.match(pattern,string)\n",
    "    if op :\n",
    "        print(\"YES\")\n",
    "    else:\n",
    "        print(\"NO\")"
