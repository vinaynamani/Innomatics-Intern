   "source": [
    "def running_sum(nums):\n",
    "    running_sum = []\n",
    "    for i in range(0,len(nums)):\n",
    "        sum_i = sum(nums[0:i+1])\n",
    "        running_sum.append(sum_i)\n",
    "    return running_sum"
   ]
